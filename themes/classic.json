  "tokenColors": [
    // Orange
    {
      "scope": ["variable.language.this", "variable.parameter"],
      "settings": {
        "foreground": "#D4AB79",
        "fontStyle": "italic"
      }
    },
    // RED_ITALIC
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#D78175",
        "fontStyle": "italic"
      }
    },
    // RED_BOLD
    {
      "scope": ["punctuation.definition.heading"],
      "settings": {
        "foreground": "#D78175",
        "fontStyle": "bold"
      }
    },
    // ORANGE:
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#AECD86",
        "fontStyle": ""
      }
    },
    // ORANGE_ITALIC
    {
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#AECD86",
        "fontStyle": "italic"
      }
    },
    // ORANGE_BOLD
    {
      "scope": [],
      "settings": {
        "foreground": "#AECD86",
        "fontStyle": "bold"
      }
    },
    // ORANGE_BOLD
    {
      "scope": [],
      "settings": {
        "foreground": "#AECD86",
        "fontStyle": "underline"
      }
    },
    // BLUE
    {
      "scope": ["support.class"],
      "settings": {
        "foreground": "#98C6B5",
        "fontStyle": ""
      }
    },
    // BLUE_UNDERLINE
    {
      "scope": ["support.type", "entity.name.type.class"],
      "settings": {
        "foreground": "#98C6B5",
        "fontStyle": ""
      }
    },
    // BLUE_ITALIC
    {
      "scope": [
        "support.constant.property-value",
        "entity.name.type",
        "storage.type.function",
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#98C6B5",
        "fontStyle": "italic"
      }
    },
    // CYAN
    {
      "scope": [
        "variable.language.super",
        "constant.character.entity",
        "punctuation.definition.entity",
        "markup.quote",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.id punctuation.definition.entity",
        "variable.parameter.url",
        "entity.name.section",
        "markup.changed",
        "meta.diff.header",
        "punctuation.definition.list",
        "source.tsx punctuation.section.embedded",
        "source.jsx punctuation.section.embedded",
        "keyword.operator",
        "storage.type.function.arrow",
        "keyword.operator.assignment",
        "punctuation.accessor.optional",
        "punctuation.definition.string.template",
        "punctuation.definition.template-expression",
        "keyword.operator.spread"
      ],
      "settings": {
        "foreground": "#D78175",
        "fontStyle": ""
      }
    },
    // CYAN_ITALIC
    {
      "scope": [
        "keyword",
        "entity.name.tag.html",
        "entity.name.tag.css",
        "keyword.operator.new",
        "storage.modifier",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#D78175",
        "fontStyle": "italic"
      }
    },
    // CYAN_UNDERLINE
    {
      "scope": [
        "link",
        "markup.underline.link",
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#D78175",
        "fontStyle": "underline"
      }
    },
    // GREEN
    {
      "scope": [
        "support.function",
        "string",
        "punctuation.definition.string",
        "constant.language.import-export-all",
        "meta.tag string",
        "markup.heading.setext",
        "meta.separator.markdown",
        "meta.property-value",
        "meta.property-value constant.other",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php",
        "markup.inserted",
        "string.template"
      ],
      "settings": {
        "foreground": "#C3BD7F",
        "fontStyle": ""
      }
    },
    // GREEN_ITALIC
    {
      "scope": [],
      "settings": {
        "foreground": "#C3BD7F",
        "fontStyle": "italic"
      }
    },
    // WHITE
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "meta.template.expression",
        "punctuation.definition.variable",
        "support.variable.property.dom",
        "support.variable.dom",
        "support.variable.property",
        "string.interpolated.pug variable",
        "punctuation",
        "meta.brace.round.js"
      ],
      "settings": {
        "foreground": "#BDC0B7"
      }
    },
    // WHITE_ITALIC
    {
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name punctuation.definition.entity",
        "markup.deleted",
        "punctuation.definition.markdown",
        "storage.modifier.pointer",
        "punctuation.definition.typeparameters",
        "punctuation.definition.directive",
        "entity.other.inherited-class",
        "punctuation.definition.entity.css",
        "entity.other.attribute-name.id.css",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#BDC0B7",
        "fontStyle": "italic"
      }
    },
    // WHITE_ITALIC
    {
      "scope": [
        "constant.character.escape",
        "variable.language.special.self",
        "meta.tag.metadata.doctype string",
        "meta.tag.sgml.doctype",
        "markup.inline.raw",
        "meta.link punctuation.definition.string",
        "meta.image punctuation.definition.string",
        "meta.diff.range",
        "constant.other.color",
        "entity.name.tag.wildcard",
        "constant",
        "support.type.property-name.css",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#AC99BF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#AC99BF",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#C692C0",
        "fontStyle": ""
      }
    },
    // WHITE_BOLD
    {
      "scope": [],
      "settings": {
        "foreground": "#BDC0B7",
        "fontStyle": "bold"
      }
    },
    // BLACK
    {
      "scope": [
        "variable.argument",
        "meta.function.parameter variable",
        "punctuation.definition.binding-pattern.array",
        "source.rust meta.type_params.rust"
      ],
      "settings": {
        "foreground": "#c9c7c2",
        "fontStyle": ""
      }
    },
    // BLACK
    {
      "scope": ["punctuation.definition.comment", "comment"],
      "settings": {
        "foreground": "#5A5A5A",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.accessor"],
      "settings": {
        "foreground": "#5A5A5A",
        "fontStyle": ""
      }
    },
    // BLACK
    {
      "scope": [
        "punctuation.definition.entity.html",
        "constant.character.entity.named.emsp.html",
        "meta.property-list entity.name.tag.css",
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "property"
      ],
      "settings": {
        "foreground": "#9eabb7",
        "fontStyle": "italic"
      }
    },
    // BLACK
    {
      "scope": [],
      "settings": {
        "foreground": "#9eabb7",
        "fontStyle": ""
      }
    },
    // BLACK
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#9eabb7",
        "fontStyle": "italic"
      }
    },
    // BLACK_BOLD
    {
      "scope": ["entity.name.section.markdown"],
      "settings": {
        "foreground": "#c9c7c2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
