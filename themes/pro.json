  "tokenColors": [
    // Orange
    {
      "scope": ["variable.language.this", "variable.parameter"],
      "settings": {
        "foreground": "#D8AE87",
        "fontStyle": "italic"
      }
    },
    // RED_ITALIC
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#BC6C9C",
        "fontStyle": "italic"
      }
    },
    // RED_BOLD
    {
      "scope": ["punctuation.definition.heading"],
      "settings": {
        "foreground": "#BC6C9C",
        "fontStyle": "bold"
      }
    },
    // ORANGE:
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#A2C282",
        "fontStyle": ""
      }
    },
    // ORANGE_ITALIC
    {
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#A2C282",
        "fontStyle": "italic"
      }
    },
    // ORANGE_BOLD
    {
      "scope": [],
      "settings": {
        "foreground": "#A2C282",
        "fontStyle": "bold"
      }
    },
    // ORANGE_BOLD
    {
      "scope": [],
      "settings": {
        "foreground": "#A2C282",
        "fontStyle": "underline"
      }
    },
    // BLUE
    {
      "scope": ["support.class"],
      "settings": {
        "foreground": "#96C8C8",
        "fontStyle": ""
      }
    },
    // BLUE_UNDERLINE
    {
      "scope": ["support.type", "entity.name.type.class"],
      "settings": {
        "foreground": "#96C8C8",
        "fontStyle": ""
      }
    },
    // BLUE_ITALIC
    {
      "scope": [
        "support.constant.property-value",
        "entity.name.type",
        "storage.type",
        "storage.type.function",
        "storage"
      ],
      "settings": {
        "foreground": "#96C8C8",
        "fontStyle": "italic"
      }
    },
    // CYAN
    {
      "scope": [
        "variable.language.super",
        "constant.character.entity",
        "punctuation.definition.entity",
        "markup.quote",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.id punctuation.definition.entity",
        "variable.parameter.url",
        "entity.name.section",
        "markup.changed",
        "meta.diff.header",
        "punctuation.definition.list",
        "source.tsx punctuation.section.embedded",
        "source.jsx punctuation.section.embedded",
        "keyword.operator",
        "storage.type.function.arrow",
        "keyword.operator.assignment",
        "punctuation.accessor.optional",
        "punctuation.definition.string.template",
        "punctuation.definition.template-expression",
        "keyword.operator.spread"
      ],
      "settings": {
        "foreground": "#BC6C9C",
        "fontStyle": ""
      }
    },
    // CYAN_ITALIC
    {
      "scope": [
        "keyword",
        "entity.name.tag.html",
        "entity.name.tag.css",
        "keyword.operator.new",
        "storage.modifier",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#BC6C9C",
        "fontStyle": "italic"
      }
    },
    // CYAN_UNDERLINE
    {
      "scope": [
        "link",
        "markup.underline.link",
        "constant.other.reference.link.markdown"
      ],
      "settings": {
        "foreground": "#BC6C9C",
        "fontStyle": "underline"
      }
    },
    // GREEN
    {
      "scope": [
        "support.function",
        "string",
        "punctuation.definition.string",
        "constant.language.import-export-all",
        "meta.tag string",
        "markup.heading.setext",
        "meta.separator.markdown",
        "meta.property-value",
        "meta.property-value constant.other",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php",
        "markup.inserted",
        "string.template"
      ],
      "settings": {
        "foreground": "#BEBB78",
        "fontStyle": ""
      }
    },
    // GREEN_ITALIC
    {
      "scope": [],
      "settings": {
        "foreground": "#BEBB78",
        "fontStyle": "italic"
      }
    },
    // WHITE
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "meta.template.expression",
        "punctuation.definition.variable",
        "support.variable.property.dom",
        "support.variable.dom",
        "support.variable.property",
        "string.interpolated.pug variable",
        "punctuation",
        "meta.brace.round.js"
      ],
      "settings": {
        "foreground": "#BFC3CA"
      }
    },
    // WHITE_ITALIC
    {
      "scope": [
        "entity.other.attribute-name.class",
        "entity.other.attribute-name punctuation.definition.entity",
        "markup.deleted",
        "punctuation.definition.markdown",
        "storage.modifier.pointer",
        "punctuation.definition.typeparameters",
        "punctuation.definition.directive",
        "entity.other.inherited-class",
        "punctuation.definition.entity.css",
        "entity.other.attribute-name.id.css",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#BFC3CA",
        "fontStyle": "italic"
      }
    },
    // WHITE_ITALIC
    {
      "scope": [
        "constant.character.escape",
        "variable.language.special.self",
        "meta.tag.metadata.doctype string",
        "meta.tag.sgml.doctype",
        "markup.inline.raw",
        "meta.link punctuation.definition.string",
        "meta.image punctuation.definition.string",
        "meta.diff.range",
        "constant.other.color",
        "entity.name.tag.wildcard",
        "constant",
        "support.type.property-name.css",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#AD93CF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#AD93CF",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#E191B7",
        "fontStyle": ""
      }
    },
    // WHITE_BOLD
    {
      "scope": [],
      "settings": {
        "foreground": "#BFC3CA",
        "fontStyle": "bold"
      }
    },
    // BLACK
    {
      "scope": [
        "variable.argument",
        "meta.function.parameter variable",
        "punctuation.definition.binding-pattern.array",
        "source.rust meta.type_params.rust"
      ],
      "settings": {
        "foreground": "#c9c7c2",
        "fontStyle": ""
      }
    },
    // BLACK
    {
      "scope": ["punctuation.definition.comment", "comment"],
      "settings": {
        "foreground": "#545460",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.accessor"],
      "settings": {
        "foreground": "#545460",
        "fontStyle": ""
      }
    },
    // BLACK
    {
      "scope": [
        "punctuation.definition.entity.html",
        "constant.character.entity.named.emsp.html",
        "meta.property-list entity.name.tag.css",
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "property"
      ],
      "settings": {
        "foreground": "#9eabb7",
        "fontStyle": "italic"
      }
    },
    // BLACK
    {
      "scope": [],
      "settings": {
        "foreground": "#9eabb7",
        "fontStyle": ""
      }
    },
    // BLACK
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#9eabb7",
        "fontStyle": "italic"
      }
    },
    // BLACK_BOLD
    {
      "scope": ["entity.name.section.markdown"],
      "settings": {
        "foreground": "#c9c7c2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
