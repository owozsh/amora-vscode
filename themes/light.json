  "tokenColors": [
    // RED
    {
      "scope": [],
      "settings": {
        "foreground": "#dc1144",
        "fontStyle": ""
      }
    },
    // RED_ITALIC
    {
      "scope": ["constant.language", "support.constant.property-value"],
      "settings": {
        "foreground": "#dc1144",
        "fontStyle": "italic"
      }
    },
    // RED_BOLD
    {
      "scope": ["punctuation.definition.heading"],
      "settings": {
        "foreground": "#dc1144",
        "fontStyle": "bold"
      }
    },
    // ORANGE:
    {
      "scope": [
        "source.tsx punctuation.section.embedded",
        "source.jsx punctuation.section.embedded",
        "punctuation.definition.template-expression",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#980874",
        "fontStyle": ""
      }
    },
    // ORANGE_ITALIC
    {
      "scope": [
        "support.type.property-name.json",
        "entity.name.tag.html",
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#980874",
        "fontStyle": "italic"
      }
    },
    // ORANGE_BOLD
    {
      "scope": ["punctuation.definition.string.template"],
      "settings": {
        "foreground": "#980874",
        "fontStyle": "bold"
      }
