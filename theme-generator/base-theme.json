      "settings": {
        "foreground": "$PROPERTY",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "$PROPERTY",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "$NUMBER",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "$NUMBER",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "$FG",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable.argument",
        "meta.function.parameter variable",
        "punctuation.definition.binding-pattern.array",
        "source.rust meta.type_params.rust"
      ],
      "settings": {
        "foreground": "#c9c7c2",
        "fontStyle": ""
      }
    },
    {
      "scope": ["punctuation.definition.comment", "comment"],
      "settings": {
        "foreground": "$COMMENT",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.accessor"],
      "settings": {
        "foreground": "$COMMENT",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "punctuation.definition.entity.html",
        "constant.character.entity.named.emsp.html",
