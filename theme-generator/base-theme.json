  "tokenColors": [
    {
      "scope": ["variable.language.this", "variable.parameter"],
      "settings": {
        "foreground": "$PARAMETER",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "$KEYWORD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.definition.heading"],
      "settings": {
        "foreground": "$KEYWORD",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "$FUNCTION",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "$FUNCTION",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "$FUNCTION",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "$FUNCTION",
        "fontStyle": "underline"
